{
  "name": "@autonav/core",
  "version": "1.6.0",
  "description": "CLI tools and Claude SDK Adapter for Autonav navigators",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "autonav": "dist/cli/autonav.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && node scripts/post-build.mjs",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "test:indicator": "npm run build && node dist/interview/ui/test-indicator.js"
  },
  "keywords": [
    "autonav",
    "navigator",
    "cli",
    "claude",
    "agent-sdk"
  ],
  "author": "Terra",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terraboops/platform-ai.git",
    "directory": "packages/autonav"
  },
  "homepage": "https://github.com/terraboops/platform-ai#readme",
  "bugs": {
    "url": "https://github.com/terraboops/platform-ai/issues"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.55",
    "@anthropic-ai/sdk": "^0.52.0",
    "@autonav/communication-layer": "*",
    "@octokit/rest": "^20.0.2",
    "@opencode-ai/sdk": "^1.2.6",
    "@slack/web-api": "^6.11.2",
    "@types/micromatch": "^4.0.10",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^14.0.2",
    "ignore": "^7.0.5",
    "ink": "^5.1.0",
    "ink-text-input": "^6.0.0",
    "micromatch": "^4.0.8",
    "ora": "^5.4.1",
    "react": "^18.3.1",
    "tar": "^6.2.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/tar": "^6.1.0",
    "ink-testing-library": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "langsmith": "^0.3.0"
  },
  "peerDependenciesMeta": {
    "langsmith": {
      "optional": true
    }
  }
}
